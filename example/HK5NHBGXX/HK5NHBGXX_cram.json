{
    "base output url": "demultiplexed/HK5NHBGXX/1",
    "import": [ "HK5NHBGXX_core.json" ],
    "molecular": [
        { "base": "@HK5NHBGXX_molecular" }
    ],
    "multiplex": {
        "algorithm": "pamld",
        "base": "@HK5NHBGXX_multiplex",
        "confidence threshold": 0.99,
        "noise": 0.03
    },
    "cellular": [
        {
            "algorithm": "mdd",
            "base": "@HK5NHBGXX_cellular",
            "quality masking threshold": 8,
            "transform": { "token": [ "0::8", "3:-8:" ] }
        },
        {
            "algorithm": "pamld",
            "base": "@HK5NHBGXX_cellular",
            "codec": {
                "@AAGAGGCATATGCAGT": { "concentration": 10 },
                "@AAGAGGCATCTACTCT": { "concentration": 2 }
            },
            "confidence threshold": 0.95,
            "noise": 0.02,
            "transform": { "token": [ "0:8:16", "3:-16:-8" ] }
        }
    ],
    "output": [ "HK5NHBGXX_l01.cram" ],
    "threads": 4
}
