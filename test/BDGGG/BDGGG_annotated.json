    ],
    "multiplex": {
        "algorithm": "pamld",
        "base": "000000000-BDGGG_multiplex",
        "codec": {
            "@AGGCAGAA": {
                "LB": "trinidad 5",
                "concentration": 1.5
            },
            "@CGTACTAG": {
                "LB": "trinidad 4",
                "concentration": 0.5
            },
            "@GGACTCCT": {
                "LB": "trinidad 9",
